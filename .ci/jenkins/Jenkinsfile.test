pipeline {
        agent {
        label 'ubuntu'
    }
    options {
        timeout(time: 10, unit: 'MINUTES')
    }

    stages {
        stage('Pre-check') {
            steps {
                sh """#!/bin/bash -el
                java --version
                mvn --version
                node --version
                go version
                pnpm --version
                """.trim()
            }
        }
    }
}
